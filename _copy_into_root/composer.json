{
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "alnv/catalog-manager-bundle": "^3.1",
    "contao/calendar-bundle": "^5.4.0",
    "contao/conflicts": "*@dev",
    "contao/listing-bundle": "^5.4.0",
    "contao/manager-bundle": "^5.4.0",
    "contao/news-bundle": "^5.4.0",
    "contao/newsletter-bundle": "^5.4.0",
    "madeyourday/contao-rocksolid-custom-elements": "^2.4",
    "oveleon/contao-component-style-manager": "^3.5",
    "oveleon/contao-cookiebar": "^2.0",
    "terminal42/contao-changelanguage": "^3.6",
    "terminal42/contao-folderpage": "^3.2",
    "terminal42/contao-leads": "^3.0",
    "terminal42/contao-node": "^1.5",
    "terminal42/notification_center": "^2.3",
    "trilobit-gmbh/contao-tiles-bundle": "^2.0",
    "vsm/vsm-helper-tools": "0.1"
  },
  "extra": {
    "public-dir": "public",
    "contao-component-dir": "assets"
  },
  "scripts": {
    "post-install-cmd": [
      "@php vendor/bin/contao-setup"
    ],
    "post-update-cmd": [
      "@php vendor/bin/contao-setup"
    ]
  },
  "repositories": [
    {
      "type": "path",
      "url": "vendor/vsm/vsm-helper-tools"
    }
  ],
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "php-http/discovery": true,
      "contao/manager-plugin": true
    }
  }
}