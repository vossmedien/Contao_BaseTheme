{
    "name": "vsm/vsm-ab-test",
    "description": "A/B Testing functionality for Contao 5.5 - by VSM",
    "keywords": [
        "contao",
        "bundle",
        "tag 3",
        "tag 4"
    ],
    "type": "contao-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "VSM Development Team",
            "email": "dev@vsm.de"
        }
    ],
    "support": {
        "issues": "https://github.com/caeli-wind/caeli-ab-test/issues",
        "source": "https://github.com/caeli-wind/caeli-ab-test"
    },
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^5.0",
        "symfony/framework-bundle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Vsm\\VsmAbTest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vsm\\VsmAbTest\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "contao/manager-plugin": true
        }
    },
    "extra": {
        "contao-manager-plugin": "Vsm\\VsmAbTest\\ContaoManager\\Plugin"
    },
    "scripts": {
        "cs-fixer": "@php tools/ecs/vendor/bin/ecs check config/ contao/ src/ templates/ tests/ --config tools/ecs/config.php --fix --ansi",
        "unit-tests": "@php tools/phpunit/vendor/bin/phpunit -c tools/phpunit/phpunit.xml.dist"
    },
    "version": "0.1"
}